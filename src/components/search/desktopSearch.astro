---
import "../../styles/search.css";

import SearchContainer from "./searchContainer.astro";
---

<SearchContainer />

<script>
	import { initDesktopSearch } from "../../scripts/desktopSearch";

	if (document.readyState === "loading") {
		document.addEventListener("DOMContentLoaded", initDesktopSearch);
	} else {
		initDesktopSearch();
	}
</script>
