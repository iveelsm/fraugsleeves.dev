{
	"name": "uchu",
	"type": "dark",
	"colors": {
		"editor.background": "#202225",
		"editor.foreground": "#e3e5e5"
	},
	"settings": [
		{
			"settings": {
				"foreground": "#e3e5e5",
				"background": "#202225"
			}
		},
		{
			"name": "Comments",
			"scope": "comment, punctuation.definition.comment",
			"settings": {
				"foreground": "#9b9b9d",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Strings",
			"scope": "string, string.quoted, string.template, punctuation.definition.string",
			"settings": {
				"foreground": "#6aa2f5"
			}
		},
		{
			"name": "Numbers",
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#3984f2"
			}
		},
		{
			"name": "Booleans and Constants",
			"scope": "constant.language, constant.language.boolean, variable.language.this, variable.language.self",
			"settings": {
				"foreground": "#3984f2"
			}
		},
		{
			"name": "Keywords",
			"scope": "keyword, keyword.control, keyword.operator.new, keyword.operator.expression, keyword.operator.logical, storage.type, storage.modifier",
			"settings": {
				"foreground": "#ea3c65"
			}
		},
		{
			"name": "Operators",
			"scope": "keyword.operator, keyword.operator.assignment, keyword.operator.arithmetic, keyword.operator.comparison, punctuation.accessor",
			"settings": {
				"foreground": "#e3e5e5"
			}
		},
		{
			"name": "Functions",
			"scope": "entity.name.function, meta.function-call, support.function",
			"settings": {
				"foreground": "#ac83de"
			}
		},
		{
			"name": "Function Parameters",
			"scope": "variable.parameter, meta.parameter",
			"settings": {
				"foreground": "#e3e5e5"
			}
		},
		{
			"name": "Classes and Types",
			"scope": "entity.name.class, entity.name.type, support.class, support.type, entity.other.inherited-class",
			"settings": {
				"foreground": "#ac83de"
			}
		},
		{
			"name": "Interfaces",
			"scope": "entity.name.type.interface",
			"settings": {
				"foreground": "#ac83de"
			}
		},
		{
			"name": "Variables",
			"scope": "variable, variable.other, variable.other.readwrite",
			"settings": {
				"foreground": "#e3e5e5"
			}
		},
		{
			"name": "Object Properties",
			"scope": "variable.other.property, variable.other.object.property, meta.object-literal.key",
			"settings": {
				"foreground": "#3984f2"
			}
		},
		{
			"name": "Punctuation",
			"scope": "punctuation, punctuation.definition.block, punctuation.definition.parameters, punctuation.separator, punctuation.terminator, meta.brace",
			"settings": {
				"foreground": "#cbcdcd"
			}
		},
		{
			"name": "Tags (HTML/JSX)",
			"scope": "entity.name.tag, punctuation.definition.tag, support.class.component",
			"settings": {
				"foreground": "#64d970"
			}
		},
		{
			"name": "Tag Attributes",
			"scope": "entity.other.attribute-name",
			"settings": {
				"foreground": "#ac83de"
			}
		},
		{
			"name": "CSS Properties",
			"scope": "support.type.property-name.css",
			"settings": {
				"foreground": "#3984f2"
			}
		},
		{
			"name": "CSS Values",
			"scope": "support.constant.property-value.css, support.constant.color",
			"settings": {
				"foreground": "#6aa2f5"
			}
		},
		{
			"name": "CSS Selectors",
			"scope": "entity.other.attribute-name.class.css, entity.other.attribute-name.id.css",
			"settings": {
				"foreground": "#64d970"
			}
		},
		{
			"name": "Regex",
			"scope": "string.regexp",
			"settings": {
				"foreground": "#6aa2f5"
			}
		},
		{
			"name": "Escape Characters",
			"scope": "constant.character.escape",
			"settings": {
				"foreground": "#64d970"
			}
		},
		{
			"name": "Markdown Headings",
			"scope": "markup.heading, entity.name.section.markdown, punctuation.definition.heading.markdown",
			"settings": {
				"foreground": "#3984f2",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markdown Bold",
			"scope": "markup.bold",
			"settings": {
				"foreground": "#e3e5e5",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markdown Italic",
			"scope": "markup.italic",
			"settings": {
				"foreground": "#e3e5e5",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Markdown Links",
			"scope": "markup.underline.link",
			"settings": {
				"foreground": "#3984f2"
			}
		},
		{
			"name": "Markdown Code",
			"scope": "markup.inline.raw, markup.fenced_code",
			"settings": {
				"foreground": "#3984f2"
			}
		},
		{
			"name": "JSON Keys",
			"scope": "support.type.property-name.json",
			"settings": {
				"foreground": "#3984f2"
			}
		},
		{
			"name": "YAML Keys",
			"scope": "entity.name.tag.yaml",
			"settings": {
				"foreground": "#3984f2"
			}
		},
		{
			"name": "Import/Export",
			"scope": "keyword.control.import, keyword.control.export, keyword.control.from, keyword.control.as",
			"settings": {
				"foreground": "#ea3c65"
			}
		},
		{
			"name": "Module Names",
			"scope": "string.quoted.module",
			"settings": {
				"foreground": "#6aa2f5"
			}
		},
		{
			"name": "Decorators",
			"scope": "meta.decorator, punctuation.decorator",
			"settings": {
				"foreground": "#ac83de"
			}
		},
		{
			"name": "TypeScript Type Annotations",
			"scope": "meta.type.annotation, entity.name.type.ts",
			"settings": {
				"foreground": "#ac83de"
			}
		}
	]
}
